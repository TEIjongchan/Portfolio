, "src/feature/layout/footer/index.jsx", "src/feature/layout/index.jsx", "src/pages/portfolio/[name]/index.jsx", "src/components/ImageSlideModal/style.js", "src/hooks/useIntersectionObserver.jsx", "src/pages/project/index.jsx", "src/feature/home/HomeHeader/index.jsx"  ],
